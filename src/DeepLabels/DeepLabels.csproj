<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>DeepLabels</PackageId>
    <Description>Common deep learning label names for .NET</Description>
    <Authors>Marcus Turewicz</Authors>
    <PackageTags>machine-learning deep-learning neural-networks image-classification object-detection imagenet coco </PackageTags>
    <!-- <PackageIcon>logo.png</PackageIcon> -->
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
    <PackageReference Include="Nerdbank.GitVersioning" Version="3.5.113" PrivateAssets="All" />
  </ItemGroup>

  <!-- <ItemGroup>
    <None Include="..\..\logo.png" Pack="true" PackagePath=""/>
  </ItemGroup> -->

</Project>
